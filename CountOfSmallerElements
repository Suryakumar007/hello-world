#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int n,count=0,max=0,t,x;

	cin>>t;	
	
	
	for(int j=0;j<t;j++)
	{
		
		cin>>n;
		int a[n];
		
		for(int i=0;i<n;i++)
		cin>>a[i];
		
		cin>>x;
		
		count=0;			
		
		for(int i=0;i<n;i++)
		{
			if(a[i]<=x)
			count++;
		}
	
	cout<<count;
		
	}
	
	
	return 0;
}
